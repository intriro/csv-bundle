{
    "name": "intriro/csv-bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "description": "Integration of Goodby CSV into Symfony2",
    "homepage": "https://github.com/intriro/csv-bundle",
    "keywords": ["csv", "goodby csv", "symfony", "bundle"],
    "authors": [{
            "name": "alsar",
            "email": "ales@intriro.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Intriro\\Bundle\\CsvBundle\\": "src"
        }
    },
    "require": {
        "php": ">=5.4",
        "symfony/framework-bundle": "^2.2 || ^3.0 || ^4.0",
        "goodby/csv": "^1.1"
    },
    "require-dev": {
      "phpunit/phpunit": "^6.0",
      "symfony/phpunit-bridge": "^2.7 || ^3.0 || ^4.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
